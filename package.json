{
  "name": "prepdd",
  "private": true,
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/app/javascript/src/__tests__/**/*.test.*"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/javascript/src/__tests__/setupTests.js"
    ],
    "roots": [
      "<rootDir>/app/javascript/src/__tests__/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },

  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "@rails/webpacker": "^4.0.7",
    "@reach/router": "^1.2.1",
    "@types/graphql": "^14.2.2",
    "@types/jest": "^24.0.16",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "apollo-boost": "^0.4.3",
    "apollo-cache": "^1.3.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-utilities": "^1.3.2",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "graphql": "^14.4.2",
    "idx": "^2.5.6",
    "jest-enzyme": "^7.0.2",
    "react": "^16.8.6",
    "react-apollo": "^3.0.0-beta.3",
    "react-dom": "^16.8.6",
    "react-google-login": "^5.0.4",
    "react-hot-loader": "^4.12.3",
    "react-linkedin-login-oauth2": "^1.0.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.2",
    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "@prettier/plugin-ruby": "^0.14.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/react-test-renderer": "^16.8.3",
    "apollo": "^2.16.0",
    "babel-jest": "^24.8.0",
    "enzyme-to-json": "^3.4.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "webpack-dev-server": "^3.7.1"
  }
}
